<%= form_for @note, html:{ id:"new-note-form" } do |f| %>
    <%= f.text_field :title, label:"Title"%>
    <%= f.text_field :subtitle, label:"Subtitle"%>
    <%= f.text_field :annotation, label:"Annotation"%>
    <%= f.text_area :body_content, label:"Body Content"%>
    <br/>
    <%= f.submit "Save Note" %>
<% end %>